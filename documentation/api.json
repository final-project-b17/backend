									"level": {
										"type": "string"
									},
									"url_group": {
										"type": "string"
									},
									"category_id": {
										"type": "integer"
									},
									"thumbnail": {
										"type": "string",
										"format": "binary"
									}
								},
								"required": ["title", "description", "price", "type_course", "level", "url_group", "category_id"]
							}
						}
					}
				},
				"responses": {
					"201": {
						"description": "Course created successfully",
						"content": {
							"application/json": {
								"example": {
									"success": true,
									"message": "Course created successfully",
									"course": {
										"id": 1,
										"title": "Sample Course",
										"description": "This is a sample course.",
										"price": 29.99,
										"type_course": "premium",
										"level": "beginner",
										"url_group": "https://example.com/group",
										"category_id": 1,
										"thumbnail": "https://example.com/thumbnail.jpg",
										"createdAt": "2023-12-01T12:34:56.789Z",
										"updatedAt": "2023-12-01T12:34:56.789Z"
									}
								}
							}
						}
					},
					"500": {
						"description": "Internal Server Error",
						"content": {
							"application/json": {
								"example": {
									"success": false,
									"error": "Internal Server Error"
								}
							}
						}
					}
				}
			}
		},
