		"/courses": {
			"get": {
				"summary": "Get a list of courses with pagination, search, and filter",
				"tags": ["Course"],
				"parameters": [
					{
						"name": "page",
						"in": "query",
						"description": "Page number for pagination",
						"schema": {
							"type": "integer",
							"default": 1
						}
					},
					{
						"name": "search",
						"in": "query",
						"description": "Search query for courses",
						"schema": {
							"type": "string"
						}
					},
					{
						"name": "rating",
						"in": "query",
						"description": "Minimum rating for courses",
						"schema": {
							"type": "integer"
						}
					},
					{
						"name": "price",
						"in": "query",
						"description": "Maximum price for courses",
						"schema": {
							"type": "number"
						}
					},
					{
						"name": "newlyReleased",
						"in": "query",
						"description": "Filter newly released courses",
						"schema": {
							"type": "boolean"
						}
					},
					{
						"name": "category",
						"in": "query",
						"description": "Category ID for filtering courses",
						"schema": {
							"type": "integer"
						}
					},
					{
						"name": "typeCourse",
						"in": "query",
						"description": "Type of course for filtering",
						"schema": {
							"type": "string"
						}
					},
					{
						"name": "level",
						"in": "query",
						"description": "Level of courses for filtering",
						"schema": {
							"type": "string"
						}
					}
				],
				"responses": {
					"200": {
						"description": "Successful response",
						"content": {
							"application/json": {
								"example": {
									"status": "success",
									"message": "Data for all courses successfully obtained!",
									"courses": [],
									"pagination": {
										"currentPage": 1,
										"pageSize": 10,
										"totalItems": 0,
										"totalPages": 0
									}
								}
							}
						}
					},
					"500": {
						"description": "Internal Server Error",
						"content": {
							"application/json": {
								"example": {
									"success": false,
									"error": "Internal Server Error"
								}
							}
						}
					}
				}
			}
		},
